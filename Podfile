# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Japanese Quran Translation' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Japanese Quran Translation
  pod 'Alamofire', '~> 4.5'

  target 'Japanese Quran TranslationTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Japanese Quran TranslationUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
